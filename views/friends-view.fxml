<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="576.0" prefWidth="828.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.guiex1.controller.FriendController">
    <children>
        <TableView fx:id="tableView" layoutX="30.0" layoutY="51.0" prefHeight="373.0" prefWidth="769.0">
            <columns>
                <TableColumn fx:id="LastName" prefWidth="375.0" text="Last Name" />
                <TableColumn fx:id="FirstName" prefWidth="393.0" text="First Name" />
            </columns>
        </TableView>
        <ButtonBar layoutX="17.0" layoutY="522.0" prefHeight="40.0" prefWidth="781.0">
            <buttons>
                <Button minWidth="0.0" mnemonicParsing="false" onAction="#handleRemove" prefHeight="60.0" prefWidth="218.0" text="Remove">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#handleAdd" prefHeight="22.0" text="Add Friend">
                    <font>
                        <Font size="16.0" />
                    </font></Button>
                <Button mnemonicParsing="false" onAction="#handleRequest" prefHeight="40.0" prefWidth="136.0" text="Request">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
            <Button mnemonicParsing="false" onAction="#handleChange" prefHeight="40.0" prefWidth="109.0" text="Change">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleOpenChat" prefHeight="39.0" text="Open Chat">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleGroupChat" prefHeight="38.0" text="Group Chat">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            </buttons>
        </ButtonBar>
      <Label fx:id="Label" layoutX="38.0" layoutY="14.0" prefHeight="40.0" prefWidth="574.0" text="Label">
         <font>
            <Font size="8.0" />
         </font></Label>
      <Button fx:id="buttonPrevious" layoutX="38.0" layoutY="447.0" mnemonicParsing="false" onAction="#handlePrevious" prefHeight="25.0" prefWidth="70.0" text="Previous" />
      <Button fx:id="buttonNext" layoutX="284.0" layoutY="447.0" mnemonicParsing="false" onAction="#handleNext" prefHeight="25.0" prefWidth="81.0" text="Next" />
      <Label fx:id="label" layoutX="126.0" layoutY="442.0" prefHeight="36.0" prefWidth="141.0" text="Label" />
      <Button fx:id="buttonProfile" layoutX="458.0" layoutY="447.0" mnemonicParsing="false" onAction="#handleOpenProfilePage" prefHeight="47.0" prefWidth="129.0" text="Profile Page" />
    </children>
</AnchorPane>
